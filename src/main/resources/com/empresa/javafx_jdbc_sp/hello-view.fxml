<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="322.0" prefWidth="337.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.empresa.javafx_jdbc_sp.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TextField fx:id="nombre" promptText="Nombre de cliente" />
   <TextField fx:id="ciudad" promptText="Dime la ciudad del cliente" />
   <TextField fx:id="facturacion" promptText="Facturacion anual del cliente" />

  <Label fx:id="welcomeText" text="..." />
  <Button onAction="#onHelloButtonClick" prefHeight="28.0" prefWidth="124.0" text="Guardar cliente" textFill="#0ccd56" />
</VBox>
